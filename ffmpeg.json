{
    "name": "ffmpeg",
    "description": "Full version of ffmpeg with libx264 encoder",
    "filters":["rfflac", "ffavf","ffbsf","ffdec","ffdmx","ffenc","ffmx","ffsws","rfmpgvid","rfnalu","mp4mx","m2tsmx","m2psdmx"],
    "help": "This filter decodes streams through ffmpeg library and is also able to encode content to h.264 streams.",
    "support": [
        "image",
        "audio",
        "video"
    ],
    "sources":"https://bevara.ddns.net/sources/ffmpeg.accessor",
    "filter_source":{
        "rfflac" : "reframe_flac.c",
        "ffavf" : "ff_avf.c",
        "ffbsf" : "ff_bsf.c",
        "ffdec" : "ff_dec.c",
        "ffdmx" : "ff_dmx.c",
        "ffenc" : "ff_enc.c",
        "ffsws" : "ff_rescale.c",
        "ffmx" : "ff_mx.c",
        "rfnalu" : "reframe_nalu.c",
        "mp4mx" : "mux_isom.c",
        "rfmpgvid" : "reframe_mpgvid.c",
        "m2tsmx" : "mux_ts.c",
        "m2psdmx" : "dmx_mpegps.c"
    }
}